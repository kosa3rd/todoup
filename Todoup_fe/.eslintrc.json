{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-essential", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error",{
       "endOfLine": "auto"
    }]
  },
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  }
}
